<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ThirdFragment"
    android:id="@+id/thirdFragmentLayout"
    android:hardwareAccelerated="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <ImageView
                android:id="@+id/rosMapZone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="0dp"
                android:layout_marginBottom="0dp"
                android:contentDescription="Map Image"
                android:gravity="center"
                android:src="@drawable/tic_final_edited" />

            <ImageButton
                android:id="@+id/btn_selectDestinationNum1"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="250dp"
                android:layout_marginTop="200dp"
                android:layout_marginEnd="60dp"
                android:background="?android:selectableItemBackground"
                android:backgroundTint="@null"
                android:clickable="true"
                android:contentDescription="goal.py"
                android:focusable="true"
                android:scaleType="fitXY"
                android:src="@drawable/lisa_location_1" />

            <ImageButton
                android:id="@+id/btn_selectDestinationNum2"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="170dp"
                android:layout_marginTop="200dp"
                android:layout_marginEnd="60dp"
                android:background="?android:selectableItemBackground"
                android:backgroundTint="@null"
                android:clickable="true"
                android:contentDescription="goal1.py"
                android:focusable="true"
                android:scaleType="fitXY"
                android:src="@drawable/lisa_location_2" />

            <ImageButton
                android:id="@+id/btn_selectDestinationNum3"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center_vertical"
                android:background="?android:selectableItemBackground"
                android:backgroundTint="@null"
                android:layout_marginStart="170dp"
                android:layout_marginTop="120dp"
                android:layout_marginEnd="60dp"
                android:clickable="true"
                android:contentDescription="goal2.py"
                android:focusable="true"
                android:scaleType="fitXY"
                android:src="@drawable/lisa_location_3" />

            <ImageButton
                android:id="@+id/btn_selectDestinationNum4"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="220dp"
                android:layout_marginTop="50dp"
                android:layout_marginEnd="50dp"
                android:background="?android:selectableItemBackground"
                android:backgroundTint="@null"
                android:clickable="true"
                android:contentDescription="goal3.py"
                android:focusable="true"
                android:scaleType="fitXY"
                android:src="@drawable/lisa_location_4" />

            <ImageButton
                android:id="@+id/btn_selectDestinationNum5"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="130dp"
                android:layout_marginTop="70dp"
                android:layout_marginEnd="60dp"
                android:background="?android:selectableItemBackground"
                android:backgroundTint="@null"
                android:clickable="true"
                android:contentDescription="pose.py"
                android:focusable="true"
                android:scaleType="fitXY"
                android:src="@drawable/lisa_home" />

        </RelativeLayout>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/call_fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="24dp"
        android:scaleX="1.1"
        android:scaleY="1.1"
        android:backgroundTint="@color/UST_yellow"
        android:focusable="true"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/person_call" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/lisa_fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="24dp"
        android:scaleX="1.1"
        android:scaleY="1.1"
        android:backgroundTint="@color/UST_yellow"
        android:focusable="true"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/lisa_ros_action_logo" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/first_action_fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="24dp"
        android:backgroundTint="@color/UST_yellow"
        android:visibility="invisible"
        android:focusable="true"
        android:clickable="true"
        app:layout_constraintBottom_toTopOf="@id/lisa_fab"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/language_icon" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/second_action_fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="24dp"
        android:backgroundTint="@color/UST_yellow"
        android:visibility="invisible"
        android:focusable="true"
        android:clickable="true"
        app:layout_constraintBottom_toTopOf="@id/first_action_fab"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/route_logo" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/third_action_fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="24dp"
        android:backgroundTint="@color/UST_yellow"
        android:visibility="invisible"
        android:focusable="true"
        android:clickable="true"
        app:layout_constraintBottom_toTopOf="@id/second_action_fab"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ic_launcher_foreground" />


</androidx.constraintlayout.widget.ConstraintLayout>